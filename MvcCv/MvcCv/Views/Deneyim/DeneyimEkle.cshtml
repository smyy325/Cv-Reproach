
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Proje Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    <input type="text" name="Title" class="form-control" placeholder="Başlığı yazınız"/>
    <br />
    <input type="text" name="Subtitle" class="form-control" placeholder="Alt Başlık"/>
    <br />
    <input type="text" name="Date" class="form-control" placeholder="Çalışma Tarihi"/>
    <br />
    <textarea placeholder="Açıklamanızı giriniz" name="Explanation" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

